    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="143dp"
        android:layout_height="93dp"
        android:layout_marginStart="250dp"
        android:layout_marginTop="560dp"
        app:srcCompat="@drawable/vendors" />

